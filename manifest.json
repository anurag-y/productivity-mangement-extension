{
    "manifest_version": 2,
    "name":"Productivity boost",
    "description": "An extension to boost productivity manyfolds",
    "version" :"1.0",
    "content_scripts":
     [
        {
          "matches": ["<all_urls>"],
         
          "js": ["content.js"]
        }
      ],
    "background":
      {
        "scripts":["background.js"]
      },
    "browser_action" :
     {
         "default_popup": "mainpopup.html",
         "default_title": "A popup"
     },
    "icons":
     { 
       "16": "fast16.png",
       "48": "fast48.png",
       "128": "fast128.png" 
      }




}